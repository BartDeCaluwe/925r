- load static
doctype 5
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(http-equiv='x-ua-compatible' content='ie=edge')

    title
      block title

    base(href='/')
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="apple-mobile-web-app-status-bar-style" content="black")
    meta(name="mobile-web-app-capable" content="yes")
    link(
      href='{% static "ninetofiver/img/touch_icon_128.png" %}'
      rel="apple-touch-icon-precomposed"
      sizes="128x128"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_192.png" %}'
      rel="apple-touch-icon-precomposed"
      sizes="192x192"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_128.png" %}'
      rel="shortcut icon"
      sizes="128x128"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_192.png" %}'
      rel="shortcut icon"
      sizes="192x192"
    )
    link(
      href='{% static "ninetofiver/img/favicon.ico" %}'
      rel="shortcut icon"
      type="image/x-icon"
    )

    block styles
      link(
        rel="stylesheet"
        href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css"
        integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M"
        crossorigin="anonymous"
      )
      link(
        href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
        rel="stylesheet"
        integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN"
        crossorigin="anonymous"
      )
      link(
        rel='stylesheet'
        href='{% static "ninetofiver/css/layout.css" %}'
      )
  body
    block body

    block scripts
      script(
        src='https://code.jquery.com/jquery-3.2.1.slim.min.js'
        integrity='sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN'
        crossorigin='anonymous'
      )
      script(
        src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js'
        integrity='sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4'
        crossorigin='anonymous'
      )
      script(
        src='https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js'
        integrity='sha256-gL1ibrbVcRIHKlCO5OXOPC/lZz/gpdApgQAzskqqXp8='
        crossorigin='anonymous'
      )
      script(
        src='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js'
        integrity='sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1'
        crossorigin='anonymous'
      )
