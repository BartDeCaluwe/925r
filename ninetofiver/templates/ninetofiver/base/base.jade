- load static
doctype 5
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(http-equiv='x-ua-compatible' content='ie=edge')

    title
      block title

    base(href='/')
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="apple-mobile-web-app-status-bar-style" content="black")
    meta(name="mobile-web-app-capable" content="yes")
    link(
      href='{% static "ninetofiver/img/touch_icon_128.png" %}'
      rel="apple-touch-icon-precomposed"
      sizes="128x128"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_192.png" %}'
      rel="apple-touch-icon-precomposed"
      sizes="192x192"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_128.png" %}'
      rel="shortcut icon"
      sizes="128x128"
    )
    link(
      href='{% static "ninetofiver/img/touch_icon_192.png" %}'
      rel="shortcut icon"
      sizes="192x192"
    )
    link(
      href='{% static "ninetofiver/img/favicon.ico" %}'
      rel="shortcut icon"
      type="image/x-icon"
    )

    link(
      rel='stylesheet'
      href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.4/css/bootstrap.min.css'
      integrity='sha384-2hfp1SzUoho7/TsGGGDaFdsuuDL0LX2hnUp6VkX3CUQ2K4K+xjboZdsXyp4oUHZj'
      crossorigin='anonymous'
    )
    link(
      rel='stylesheet'
      href='https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css'
      integrity='sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1'
      crossorigin='anonymous'
    )
    link(
      rel='stylesheet'
      href='{% static "ninetofiver/css/layout.css" %}'
    )

    block styles
  body
    block body

    script(
      src='https://ajax.googleapis.com/ajax/libs/jquery/3.0.0/jquery.min.js'
      integrity='sha384-THPy051/pYDQGanwU6poAc/hOdQxjnOEXzbT+OuUAFqNqFjL+4IGLBgCJC3ZOShY'
      crossorigin='anonymous'
    )
    script(
      src='https://cdnjs.cloudflare.com/ajax/libs/tether/1.2.0/js/tether.min.js'
      integrity='sha384-Plbmg8JY28KFelvJVai01l8WyZzrYWG825m+cZ0eDDS1f7d/js6ikvy1+X+guPIB'
      crossorigin='anonymous'
    )
    script(
      src='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.4/js/bootstrap.min.js'
      integrity='sha384-VjEeINv9OSwtWFLAtmc4JCtEJXXBub00gtSnszmspDLCtC0I4z4nqz7rEFbIZLLU'
      crossorigin='anonymous'
    )

    block scripts
